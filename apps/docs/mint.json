{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Cubent Coder",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#000000",
    "light": "#404040",
    "dark": "#000000",
    "background": {
      "light": "#ffffff",
      "dark": "#0a0a0a"
    },
    "anchors": {
      "from": "#000000",
      "to": "#404040"
    }
  },
  "theme": "minimal",
  "layout": "topnav",
  "modeToggle": {
    "default": "light",
    "isHidden": false
  },
  "topbarLinks": [
    {
      "name": "Website",
      "url": "https://cubent.com"
    },
    {
      "name": "GitHub",
      "url": "https://github.com/LaxBloxBoy2/Cubent"
    },
    {
      "name": "Discord",
      "url": "https://discord.gg/cubent"
    }
  ],
  "topbarCtaButton": {
    "name": "Download",
    "url": "https://marketplace.visualstudio.com/items?itemName=cubent.cubent"
  },
  "tabs": [
    {
      "name": "Guide",
      "url": "guide"
    },
    {
      "name": "Features",
      "url": "features"
    },
    {
      "name": "API",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "VS Code Extension",
      "icon": "download",
      "url": "https://marketplace.visualstudio.com/items?itemName=cubent.cubent"
    },
    {
      "name": "Discord Community",
      "icon": "discord",
      "url": "https://discord.gg/cubent"
    },
    {
      "name": "GitHub Repository",
      "icon": "github",
      "url": "https://github.com/LaxBloxBoy2/Cubent"
    }
  ],
  "navigation": [
    {
      "group": "Guide",
      "pages": [
        "introduction",
        "installation",
        "first-steps",
        "models-and-pricing",
        {
          "group": "Advanced",
          "pages": [
            "advanced/mcp-integration",
            "advanced/local-models",
            "advanced/custom-instructions",
            "advanced/automation"
          ]
        }
      ]
    },
    {
      "group": "Features",
      "pages": [
        "features/chat-mode",
        "features/agent-mode",
        "features/custom-modes",
        "features/file-operations",
        "features/terminal-integration"
      ]
    },
    {
      "group": "API",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Community",
      "pages": [
        "community/discord",
        "community/contributing",
        "community/troubleshooting"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/LaxBloxBoy2/Cubent",
    "discord": "https://discord.gg/cubent",
    "reddit": "https://www.reddit.com/r/cubent/"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "search": {
    "prompt": "Search documentation..."
  },
  "api": {
    "baseUrl": "https://api.cubent.com",
    "auth": {
      "method": "bearer"
    }
  }
}
