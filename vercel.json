{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dependabot/**": false
    }
  },
  "github": {
    "autoAlias": false
  },
  "buildCommand": "chmod +x scripts/vercel-build.sh && ./scripts/vercel-build.sh",
  "installCommand": "pnpm install",
  "functions": {
    "apps/web/**/*.ts": {
      "includeFiles": "packages/database/prisma/schema.prisma"
    }
  }
}
